@model Producto

<h1>Registrar Producto</h1>

<form method="post">
    <table class="table">
    <div class="form-group">
        <label asp-for="Nombre"></label>
        <input asp-for="Nombre" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="Foto"></label>
        <input asp-for="Foto" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="Descripción"></label>
        <input asp-for="Descripción" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="Precio"></label>
        <input asp-for="Precio" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="Celular"></label>
        <input asp-for="Celular" class="form-control">
    </div>
     <div class="form-group">
        <label asp-for="Lugar"></label>
        <input asp-for="Lugar" class="form-control">
    </div>
     <div class="form-group">
        <label asp-for="Categoria"></label>
        <input asp-for="Categoria" class="form-control">
    </div>
    <button class="btn btn-primary">Enviar</button>
</form>
</table>